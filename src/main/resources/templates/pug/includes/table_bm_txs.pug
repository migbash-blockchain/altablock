.container
    .container(style="display: flex; align-items: center;")
        svg(vwidth="22" height="20" viewBox="0 0 22 20")
                use(th:xlink:href="@{/images/Vectors.svg#histroy_vector}")
        span.p-2
            strong(style="font-size: 32px;") Transaction History
    table#bt_tx_table.table.table-borderless.m-3
        thead
            tr
                each n in ["Transaction ID", "Sender", "Recipient", "Value", "Block Number", "Elapsed Time"]
                    th(scope="col")= n
        tbody#contents
            tr(th:each="list_value : ${bm_all_transactions}")
                td(th:text="${list_value.getTransactionId()}")
                td(th:text="${@stringUtil.getStringFromKey(list_value.getSender())}")
                td(th:text="${@stringUtil.getStringFromKey(list_value.getRecipient())}")
                td(th:text="${list_value.getValue()}")
                td(th:text="${list_value.getBlockNumber()}")
                td(th:text="${list_value.getBlockNumber()}")